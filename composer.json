{
	"name": "neftaio/laravel-jmespath",
	"description": "JmesPath Collection Macro",
	"keywords": [
		"jmespath",
		"laravel",
		"jmes"
	],
	"license": "MIT",
	"authors": [{
		"name": "Neftali Forero",
		"email": "neftaio@gmail.com"
	}],
	"require": {
		"php": "^7.4.0",
		"mtdowling/jmespath.php": "~2.6",
		"illuminate/support": "8.*",
		"illuminate/config": "8.*",
		"illuminate/console": "8.*",
		"illuminate/filesystem": "8.*",
		"illuminate/container": "8.*"
	},
	"autoload": {
		"psr-4": {
			"Neftaio\\Jmes\\": "src/"
		},
		"files": [
			"src/helpers.php"
		]
	},
	"extra": {
		"branch-alias": {
			"dev-master": "dev"
		},
		"laravel": {
			"providers": [
				"Neftaio\\Jmes\\JmesServiceProvider"
			],
			"aliases": {
				"Jmes": "Neftaio\\Jmes\\Facades\\Jmes"
			}
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
